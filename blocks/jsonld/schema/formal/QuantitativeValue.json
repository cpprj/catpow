{
    "$schema": "../schema.json",
    "name": "QuantitativeValue",
    "label": "時間単価",
    "url": "https://schema.org/QuantitativeValue",
    "items": [
        {"name": "value","label": "価格","input": "number"},
        {"name": "unitText","label": "単位時間","input": "select","options": [
            {"label":"時間","value":"HOUR"},
            {"label":"日","value":"DAY"},
            {"label":"週","value":"WEEK"},
            {"label":"月","value":"MONTH"},
            {"label":"年","value":"YEAR"}
        ],"default": "HOUR"}
    ]
}